<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.exam.sp.Exam">
		<select id="selectExamList" resultType="com.bdi.test.vo.ExamVo">
			select * from exam
		</select>
		
		<select id="selectExam" resultType="com.bdi.test.vo.ExamVo">
			select * from exam
			where num=#{num}
		</select>
	</mapper>